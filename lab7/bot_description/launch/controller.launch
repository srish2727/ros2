<launch>

<rosparam file="cat '$(find bot_description)/launch/controller.yaml'" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="bot" args="Rev1_position_controller Rev2_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/bot/joint_states"/>
</node>

</launch>